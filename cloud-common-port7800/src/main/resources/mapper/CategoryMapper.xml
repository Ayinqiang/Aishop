<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.base.common.dao.CategoryMapper">


    <select id="getAllCategory" resultType="com.base.pojo.Category">
        SELECT class_code as classCode, class_name as className, icon, color FROM category;
    </select>


</mapper>
